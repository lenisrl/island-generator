{
  "name": "island-generator",
  "version": "1.0.0",
  "main": "dist/main/main.js", 
  "scripts": {
    "dev": "concurrently -k \"cross-env BROWSER=none npm run dev:react\" \"npm run dev:electron\"",
    "dev:react": "vite",
    "dev:electron": "tsc -p tsconfig.json && electron .",
    "build": "tsc -p tsconfig.json && vite build"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.0.0",
    "concurrently": "^8.0.0",
    "cross-env": "^7.0.3",
    "electron": "^25.0.0", 
    "electron-builder": "^24.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0",
    "vite": "^4.0.0"
  }
}